                                },
							"created_date_month": {
                                "schema" : "manudevelopia$uqasarcube",
                                "table" : "jira_full",
                                "column" : "Created",
                                "extract":"month"
                                },
							"created_date_day": {
                                "schema" : "manudevelopia$uqasarcube",
                                "table" : "jira_full",
                                "column" : "Created",
                                "extract":"day"},
                          	"id" : {
		                        "schema" : "manudevelopia$uqasarcube",
		                        "table" : "jira_full",
		                        "column" : "Key" 
                    		},
                            "issue_avg" : {
